main.viewArea2D.navigateTo();

int players = main.n_players;
double lambda = sqrt(main.tables.size() / 15.0);
int columns = (int)(ceil(5.0 * lambda));
int rows = (int)(main.tables.size() / columns);
double h_spacing = 45, v_spacing = 50;
int vOffset = (int)((main.floor.getHeight() - (v_spacing * rows)) / 2);
int hOffset = (int)((main.floor.getWidth() - (h_spacing * columns)) / 2);

for (int i = 0; i < main.tables.size(); i++) {
    int row = i / columns;
    int col = i % columns;
    
    double x = hOffset + col * h_spacing + main.floor.getX();
    double y = vOffset + row * v_spacing + main.floor.getY();
    
    main.tables.get(i).setXY(x, y);
    
}
]]></Action>
