					<Body><![CDATA[turn++;
ArrayList<Player> players = new ArrayList();
for(int i=0; i<main.n_players; i++){
	players.add(main.players.get(i));
}
//System.out.println(players.size());
Collections.sort(players, (o1, o2) -> o2.getScore() - o1.getScore());

//print top 16/////
System.out.println("---------------------------");
System.out.println("TOP" + main.top + " turn " + turn);
System.out.println("---------------------------");
for(int i = 0; i < main.top; i++){
	System.out.println(players.get(i));
}
System.out.println("---------------------------");

///////////////////

for(int table = 0; table< main.tables.size(); table++){
	if(players.size() ==  0) {System.out.println(table);break;}
	Player p1 = players.remove(0), p2 = null;
	for (int i= 0; i< players.size(); i++){
		if (!p1.opponents.contains(players.get(i))){ 
			p2 = players.remove(i);
			break;	
		}
		
		if(p2 == null && i == players.size() -1){
			p2 = players.remove(0);
		}
	}
	
	p1.opponents.add(p2);
	p2.opponents.add(p1);
	
	String output = "table "+ table +", p1 "+ p1.toString()+" p2 "+ p2.toString();
	//System.out.println(output);
	send(main.tables.get(table), p1);
	send(main.tables.get(table), p2);
	//System.out.println(players.size());
}
