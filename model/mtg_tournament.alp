						<EntryAction><![CDATA[int top = min(8, main.top); 
turn = 1;

ArrayList<Player> players = new ArrayList();
for(int i=0; i<main.n_players; i++){
	players.add(main.players.get(i));
}
Collections.sort(players, (o1, o2) -> o2.getScore() - o1.getScore());

//print the topx players
main.output.println("**************************************************************************");
for(int i = 0; i < main.top; i++){
	Player p = players.get(i);
	String out = ""+ (i+1) + ": "
			+ main.externalMatchup.getCellStringValue(1, 1, p.deck+1) 
			+ ": " + p.matches_won 
			+ "-" + p.matches_lost 
			+ "-" + p.matches_draw;
	main.swiss_text = main.swiss_text + out + "\n";
	System.out.println(out);
	main.output.println(out);
}


for(int i = 0; i< main.top; i++){
	Player p = players.get(i);
	Color color = new Color(Integer.parseInt(main.externalMetashare.getCellStringValue(1, p.deck + 1, 4), 16));
	Pair<String, Color> deckAndColor = new Pair<String, Color>(main.externalMatchup.getCellStringValue(1, 1, p.deck+1), color);
	main.top_decks[i] = deckAndColor;
}

//remove all players that are not in the top and all the useless tables
for(int i = top; i < main.n_players; i++){
	main.remove_players(players.remove(top));
}
int s = main.tables.size();
for(int i = top/2; i < s; i++){
	main.remove_tables(main.tables.get(top/2));
}

//the pairings are:
for(int i = 0; i<top/2; i++){
	send(main.tables.get(i), players.get(i));
	send(main.tables.get(i), players.get(top - i - 1));
}]]></EntryAction>
