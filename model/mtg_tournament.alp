main.text3D.setY((main.text3D.getY() - (main.viewArea2D.getY() + main.hCyan1.getHeight())) * c + (main.viewArea2D.getY() + (int)(main.hCyan1.getHeight()* c)));

main.textStatistics.setX(main.text3D.getX() + (int)(main.hCyan1.getWidth() * c));
main.textStatistics.setY(main.text3D.getY());

main.hCyan1.setHeight((int)(main.hCyan1.getHeight() *c));
main.hCyan1.setWidth((int)(main.hCyan1.getWidth() * c));

